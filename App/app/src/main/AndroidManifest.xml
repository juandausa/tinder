<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tinder_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.DesignDemo">

        <activity android:name="com.tinder_app.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.tinder_app.UserProfileActivity"
            android:parentActivityName="com.tinder_app.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="com.tinder_app.MainActivity" />
        </activity>

    </application>

</manifest>
